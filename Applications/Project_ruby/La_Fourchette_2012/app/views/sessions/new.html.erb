<h1>Log In</h1>
<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag :username %>
		<%= text_field_tag :username, params[:username] %>
	</div>
	<div class="field">
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</div>
	
	<%= submit_tag "Se connecter" %>
<% end %>

